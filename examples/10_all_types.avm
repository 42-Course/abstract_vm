; Example 10: All Operand Types
; Demonstrates all five operand types in AbstractVM

push int8(127)        ; 8-bit integer (max value)
push int16(32767)     ; 16-bit integer (max value)
push int32(2147483647); 32-bit integer (max value)
push float(3.14159)   ; Single precision float
push double(2.71828)  ; Double precision float

; Display all values on stack
dump

; Clean up stack
pop
pop
pop
pop
pop

exit
